  <UserControl x:Class="PointOfSale.MenuCategoryWindows.SidesMenu"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
             xmlns:local="clr-namespace:PointOfSale.MenuCategoryWindows"
             mc:Ignorable="d" 
             d:DesignHeight="450" d:DesignWidth="500">
    <Grid>
        <Grid.ColumnDefinitions>
        </Grid.ColumnDefinitions>
        <Grid.RowDefinitions>
            <RowDefinition/>
            <RowDefinition/>
            <RowDefinition/>
            <RowDefinition/>
            <RowDefinition/>
        </Grid.RowDefinitions>
        <Button Grid.Row="0" Margin="10" Click="FriesButton" FontSize="30" FontWeight="Bold">Dragonborn Waffle Fries</Button>
        <Button Grid.Row="1" Margin="10" Click="MiraakButton" FontSize="30" FontWeight="Bold">Fried Miraak</Button>
        <Button Grid.Row="2" Margin="10" Click="GritsButton" FontSize="30" FontWeight="Bold">Mad Otar Grits</Button>
        <Button Grid.Row="3" Margin="10" Click="SaladButton" FontSize="30" FontWeight="Bold">Vokun Salad</Button>
        <Button Grid.Row="4" Margin="10" Click="SidesBackButton" FontSize="30" FontWeight="Bold">Back</Button>
    </Grid>
</UserControl>
